{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 18,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.lineHeight": 28,
  "editor.fontLigatures": "'ss01','ss05','dlig'",
  "window.zoomLevel": 1,
  "files.autoSave": "onFocusChange",
  "cSpell.words": [
    "enddate",
    "juejin",
    "suibianshenme",
    "tinyint",
    "typeorm"
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/target": true,
    "**/logs": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/node_modules": true
  }
}
